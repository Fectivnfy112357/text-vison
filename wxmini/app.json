{
  "pages": [
    "pages/index/index",
    "pages/create/create",
    "pages/history/history",
    "pages/templates/templates",
    "pages/profile/profile",
    "pages/detail/detail",
    "pages/login/login"
  ],
  "subpackages": [
    {
      "root": "subpages",
      "name": "sub",
      "pages": [
        "template-detail/template-detail",
        "settings/settings",
        "about/about"
      ]
    }
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#ffffff",
    "navigationBarTitleText": "文生视界",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#f8f9fa",
    "enablePullDownRefresh": false,
    "onReachBottomDistance": 50
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#3cc51f",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "position": "bottom",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "assets/icons/home.png",
        "selectedIconPath": "assets/icons/home-active.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/create/create",
        "iconPath": "assets/icons/create.png",
        "selectedIconPath": "assets/icons/create-active.png",
        "text": "创作"
      },
      {
        "pagePath": "pages/templates/templates",
        "iconPath": "assets/icons/template.png",
        "selectedIconPath": "assets/icons/template-active.png",
        "text": "模板"
      },
      {
        "pagePath": "pages/history/history",
        "iconPath": "assets/icons/history.png",
        "selectedIconPath": "assets/icons/history-active.png",
        "text": "历史"
      },
      {
        "pagePath": "pages/profile/profile",
        "iconPath": "assets/icons/profile.png",
        "selectedIconPath": "assets/icons/profile-active.png",
        "text": "我的"
      }
    ]
  },
  "networkTimeout": {
    "request": 10000,
    "downloadFile": 10000,
    "uploadFile": 10000
  },
  "debug": false,
  "navigateToMiniProgramAppIdList": [],
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  },
  "requiredBackgroundModes": ["audio"],
  "plugins": {},
  "resizable": true,
  "usingComponents": {},
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "darkmode": false,
  "themeLocation": "theme.json",
  "renderer": "skyline",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "disableABTest": false,
      "sdkVersionBegin": "3.9.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "componentFramework": "glass-easel",
  "style": "v2"
}